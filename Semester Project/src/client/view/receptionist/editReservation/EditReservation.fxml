<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/16"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="client.view.receptionist.editReservation.EditReservationController">
    <children>
        <Label layoutX="21.0" layoutY="78.0" text="New Start Date :"/>
        <Label layoutX="23.0" layoutY="133.0" text="New End Date :"/>
        <DatePicker fx:id="dateFrom" layoutX="124.0" layoutY="74.0"/>
        <DatePicker fx:id="dateTo" layoutX="123.0" layoutY="129.0"/>
        <Label layoutX="338.0" layoutY="53.0" text="Available Rooms :"/>
        <ListView fx:id="roomList" layoutX="338.0" layoutY="78.0"
                  prefHeight="200.0" prefWidth="175.0"/>
        <Button layoutX="359.0" layoutY="338.0" mnemonicParsing="false"
                onAction="#onUpdate" text="Update"/>
        <Button layoutX="448.0" layoutY="338.0" mnemonicParsing="false"
                text="Cancel"/>
        <Button layoutX="104.0" layoutY="213.0" mnemonicParsing="false"
                onAction="#onSearch" text="Search"/>
        <ComboBox fx:id="userType" layoutX="177.0" layoutY="187.0"
                  prefWidth="150.0"/>
        <Label layoutX="197.0" layoutY="169.0" text="Room Type :"/>
        <Label fx:id="error" layoutX="350.0" layoutY="292.0" prefHeight="18.0"
               prefWidth="235.0" text="Label" textFill="RED"/>
        <Label layoutX="45.0" layoutY="314.0" text="New Room :"/>
        <Label fx:id="newRoom" layoutX="116.0" layoutY="314.0" prefHeight="18.0"
               prefWidth="66.0" text="Label"/>
    </children>
</AnchorPane>
