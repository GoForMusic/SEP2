<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<AnchorPane fx:id="anchorpane" prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/16"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="client.view.customer.doubleBedroom.CustomerDoubleBedRoomController">
    <children>
        <DatePicker fx:id="customerDoubleDateFrom" layoutX="14.0"
                    layoutY="119.0" prefHeight="25.0" prefWidth="165.0"
                    promptText="start date"/>
        <DatePicker fx:id="customerDoubleDateTo" layoutX="14.0" layoutY="188.0"
                    prefHeight="25.0" prefWidth="165.0" promptText="end date"/>
        <HBox layoutX="190.0" prefHeight="80.0" prefWidth="420.0"
              style="-fx-background-color: #907cd6;">
            <children>
                <Label alignment="CENTER" contentDisplay="CENTER"
                       text="Double Bed Room">
                    <font>
                        <Font name="Californian FB" size="26.0"/>
                    </font>
                </Label>
            </children>
        </HBox>
        <ImageView fitHeight="159.0" fitWidth="370.0" layoutX="215.0"
                   layoutY="97.0">
            <image>
                <Image url="@107219189-.jpg"/>
            </image>
        </ImageView>
        <Label layoutX="482.0" layoutY="349.0" text="Label">
            <font>
                <Font size="15.0"/>
            </font>
        </Label>
        <Button layoutX="36.0" layoutY="322.0" mnemonicParsing="false"
                onAction="#searchRooms" prefHeight="37.0" prefWidth="146.0"
                text="Search Rooms">
            <font>
                <Font size="15.0"/>
            </font>
        </Button>
        <Label layoutX="14.0" layoutY="97.0" text="Book From :"/>
        <Label layoutX="14.0" layoutY="168.0" text="Book to :"/>
        <TextArea fx:id="description" layoutX="224.0" layoutY="262.0"
                  prefHeight="66.0" prefWidth="361.0"/>
        <Label layoutX="14.0" layoutY="239.0" text="Price per night :"/>
        <Label fx:id="price" layoutX="109.0" layoutY="239.0" prefHeight="18.0"
               prefWidth="47.0" text="price"/>
    </children>
</AnchorPane>
